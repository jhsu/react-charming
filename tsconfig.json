{
  "compilerOptions": {
    "outDir": "./lib/",
    "strictNullChecks": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "jsx": "react",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "target": "es6",
    "lib": [
      "dom",
      "es7"
    ],
    "typeRoots": ["node_modules/@types", "./typings"]
  },
  "exclude": [
    "node_modules",
    "lib",
  ]
}
